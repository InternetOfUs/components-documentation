		{
			"name": "Create Profile",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "da5374c7-1e50-41b0-b101-d4a2d5b49c7f",
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.collectionVariables.set(\"profileId\", jsonData.id);",
							"pm.collectionVariables.set(\"creationTs\", jsonData._creationTs);",
